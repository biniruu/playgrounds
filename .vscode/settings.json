{
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.autoClosingBrackets": "always",
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "autoimport.useSemiColon": false, // auto import 기능으로 import한 모듈에 세미콜론 삽입
  "aws.codeWhisperer.shareCodeWhispererContentWithAWS": false,
  "aws.telemetry": false,
  "bookmarks.saveBookmarksInProject": true,
  "bookmarks.keepBookmarksOnLineDelete": true,
  "breadcrumbs.enabled": true,
  "color-highlight.markRuler": false, // minimap에서 color-highlight 활성화
  "css.customData": ["./css_custom_data.json"], // at-rule 등 css 관련 규칙을 사용자화 하여 이용하는 데 필요한 설정 파일을 연결하는 옵션
  "css.lint.unknownAtRules": "ignore", // sass 등과 같은 css 전처리기에서 사용하는 at rule을 css 파일에서 사용했을 때 at-rule-no-unknown 경고가 발생하지 않도록 함. stylelint가 발생시키는 에러 처리는 해당 at rule을 .stylelintrc에서 'at-rule-no-unknown'에 ignoreAtRules 옵션으로 추가할 것
  "css.lint.zeroUnits": "warning",
  "docwriter.progress.trackClasses": true,
  "docwriter.progress.trackTypes": true,
  "editor.bracketPairColorization.enabled": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll.markdownlint": true,
    "source.fixAll.stylelint": true,
    "source.organizeImports": true // 사용하지 않는 import 제거
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.definitionLinkOpensInPeek": true,
  "editor.fontFamily": "D2Coding, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 16,
  "editor.formatOnSave": true, // 저장할 때마다 포맷을 자동으로 교정
  "editor.hover.delay": 600,
  "editor.inlineSuggest.enabled": true,
  "editor.linkedEditing": true,
  "editor.minimap.enabled": true,
  "editor.minimap.showSlider": "always",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.renderWhitespace": "all",
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    // "comments": "#fff" // 주석 글자 색
  },
  "editor.unfoldOnClickAfterEndOfLine": true,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact", // jsx 또는 js에서 emmet을 사용하는 옵션
    "postcss": "css" // postcss 문법을 사용하기 위한 옵션
  },
  "emmet.preferences": {
    "css.color.short": true
  },
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.syntaxProfiles": {
    "postcss": "css" // postcss 문법을 사용하기 위한 옵션
  },
  "emmet.triggerExpansionOnTab": true,
  "eslint.alwaysShowStatus": true,
  "eslint.format.enable": true,
  "eslint.packageManager": "yarn",
  "eslint.workingDirectories": [{ "mode": "auto" }], // .eslintrc 파일 위치
  "explorer.autoReveal": false,
  "explorer.compactFolders": false, // 하위 폴더가 하나밖에 없을 때 폴더 정렬이 일렬로 나오지 않고 일반적인 방식으로 계층화 되도록 함
  "explorer.confirmDelete": false, // 파일 삭제 전 확인 모달 띄우기
  "explorer.confirmDragAndDrop": false, // 파일 드래그 & 드롭 전 확인 모달 띄우기
  "files.associations": {
    "*.css": "postcss"
  },
  "files.exclude": {
    "**/.git": false
  }, // 사이드바에서 파일/폴더 감추기
  "files.insertFinalNewline": true,
  "git.autofetch": true, // 일정 시간마다 default 브랜치를 자동으로 fetch 하기
  "git.fetchOnPull": true,
  "git.showPushSuccessNotification": true,
  "githubPullRequests.fileListLayout": "tree",
  "github.copilot.enable": {
    "*": true,
    "markdown": false,
    "plaintext": false,
    "vue": false,
    "yaml": false
  },
  "githubNotificationsBell.oauthToken": "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",
  "gitlens.gitCommands.search.showResultsInSideBar": true,
  "html-css-class-completion.enableEmmetSupport": true,
  "jest.autoRun": {
    "watch": true
  },
  "jest.showTerminalOnLaunch": false, // jest 익스텐션을 실행했을 때 자동으로 콘솔 창 띄우기
  "jest.testExplorer": {
    "enabled": true,
    "showClassicStatus": true,
    "showInlineError": true
  },
  "jestTestExplorer.flattenExplorer": true,
  "liveServer.settings.AdvanceCustomBrowserCmdLine": "",
  "liveServer.settings.ChromeDebuggingAttachment": true,
  "liveServer.settings.donotShowInfoMsg": true,
  "npm.keybindingsChangedWarningShown": true,
  "git.mergeEditor": true, // conflict 시 diff 화면 스타일을 3-way merge로 표시
  "problems.autoReveal": false,
  "projectManager.git.baseFolders": ["/Users/biniruu/Projects", "/Users/biniruu/Learning"],
  "python.jediEnabled": false,
  "pythonPreview.codeFooterDocs.fontSize": 13,
  "quokka.automaticRestart": true,
  "redhat.telemetry.enabled": false,
  "stylelint.validate": [
    "css",
    "html",
    "less",
    "postcss",
    "sass",
    "scss",
    "source.css.styled",
    "source.markdown.math",
    "styled-css",
    "sugarss",
    "svelte",
    "typescript",
    "typescriptreact",
    "vue",
    "vue-html",
    "vue-postcss",
    "xml",
    "xsl"
  ], // stylelint로 유효성을 검증할 언어만 선택
  "turboConsoleLog.logMessagePrefix": "📝",
  "turboConsoleLog.quote": "'",
  "turboConsoleLog.includeFileNameAndLineNum": false,
  "turboConsoleLog.delimiterInsideMessage": "--",
  "turboConsoleLog.insertEnclosingFunction": false,
  "turboConsoleLog.insertEnclosingClass": false,
  "terminal.integrated.defaultProfile.linux": "/bin/zsh",
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.fontFamily": "D2Coding, Menlo, Monaco, 'Courier New', monospace",
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.lineHeight": 1.4,
  "testExplorer.useNativeTesting": true,
  "todo-tree.tree.showScanModeButton": false,
  "todohighlight.isEnable": true,
  "typescript.tsserver.pluginPaths": ["typescript-plugin-css-modules"],
  // "typescript.validate.enable": false, // typescript를 사용하지 않는 프로젝트에서 typescript 관련 오류가 발생할 때 활성화
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "workbench.colorCustomizations": {
